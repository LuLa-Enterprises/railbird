{
  "expo": {
    "name": "Railbird",
    "slug": "railbird-handicapping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E40AF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.railbird.handicapping"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E40AF"
      },
      "package": "com.railbird.handicapping"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Railbird - AI Horse Racing Handicapper",
      "shortName": "Railbird",
      "description": "AI-powered conversational handicapping assistant for horse racing",
      "themeColor": "#1E40AF",
      "backgroundColor": "#1E40AF"
    },
    "plugins": [
      "expo-document-picker",
      "expo-image-picker",
      "expo-av",
      "expo-speech"
    ],
    "scheme": "railbird",
    "extra": {
      "eas": {
        "projectId": "railbird-handicapping"
      }
    }
  }
}
